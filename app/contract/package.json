{
  "name": "@midnight-ntwrk/privaterent-contract",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "compact": "compactc compile src/PrivateRent.compact src/build/",
    "compactc": "compactc compile src/PrivateRent.compact src/build/",
    "build": "rm -rf dist && tsc --project tsconfig.build.json && cp -Rf ./src/build ./dist/build && cp ./src/PrivateRent.compact ./dist",
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  }
}
